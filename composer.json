{
  "name": "wp-oop/plugin-boilerplate",
  "description": "A project skeleton useful for starting a new WordPress plugin",
  "minimum-stability": "dev",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Anton Ukhanev",
      "email": "xedin.unknown@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2 | ^8.0",
    "dhii/module-interface": "^0.3-alpha1",
    "psr/container": "^1.0@stable",
    "dhii/containers": "^0.1",
    "wikimedia/composer-merge-plugin": "^2.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 | ^8.0 | ^9.0",
    "johnpbloch/wordpress-core": "^5.0@stable",
    "vimeo/psalm": "^3.11.7 | ^4.0",
    "slevomat/coding-standard": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Me\\Plugin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Me\\Plugin\\Test\\": "tests"
    }
  },
  "extra": {
    "merge-plugin": {
      "include": [
        "modules.local/*/composer.json"
      ]
    },
    "branch-alias": {
      "dev-initial": "0.1.x-dev"
    }
  }
}
